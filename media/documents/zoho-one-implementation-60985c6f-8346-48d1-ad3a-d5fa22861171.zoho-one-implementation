	<script type="text/javascript">
		/* navigation section */
		$(function () { $("#navigation").load("navigation-two.html"); });
		/* portfolio section */
		$(function () { $("#portfolio").load("portfolio-common.html"); });
		/* our partner section */
		$('.more-less').click(function () {
			var $this = $(this);
			$this.toggleClass('more-less');
			if ($this.hasClass('more-less')) {
				$this.text('View More');
			} else {
				$this.text('View Less');
			}
		});
		/* footer section */
		$(function () { $("#footer").load("footer.html"); });
		/* schedule a call */
		$("#schedule-btn").click(function () {
			$("#call-iframe").attr('src', 'https://bizappln.zohobookings.com/');
		});
		$("#schedule-btn1").click(function () {
			$("#call-iframe").attr('src', 'https://bizappln.zohobookings.com/');
		});
		/* testimonial section */
		$(function () { $("#testimonial").load("testimonial-common.html"); });
		/* Award section */
		$(function () { $("#awards").load("awards-common.html"); });
		/* clutch section */
		$(function () { $("#clutch_widget").load("clutch_widget_common.html"); });
		/* preloader */
		$(function () {
			$("#loading").fadeOut(7000, function () {
				$(".modal-body").fadeIn(1000);
			});
		});

	</script>
	
